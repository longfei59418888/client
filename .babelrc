{
  "presets": [
    "react",
    "react-optimize",
    [
      "env",
      {
        "modules": false,
        "debug": true,
        "useBuiltIns": false,
        "targets": {
          "browsers": "defaults"
        }
      }
    ],
    "stage-0"
  ],
  "plugins": [
    [
      "import",
      {
        "libraryName": "zarm",
        "style": false
      }
    ],
    "transform-async-to-generator",
    "transform-decorators-legacy",
    "transform-es2015-modules-commonjs",
    "syntax-dynamic-import",
    [
      "transform-runtime",
      {
        "helpers": false,
        "polyfill": false,
        "regenerator": true,
        "moduleName": "babel-runtime"
      }
    ]
  ]
}